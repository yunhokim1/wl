<body style="background-color: #866ec7;">


  <div class="container-xl px-4 mt-5">
    <div class="row justify-content-center">
      <div class="col-lg-7">
        <div class="card shadow-lg border-0 rounded-lg mt-5 ">
          <div class="card-header justify-content-center">
            <h3 class="fw-light my-4 ms-3">비밀번호 찾기</h3>
          </div>
          <div class="card-body d-flex justify-content-center px-5">

            <table class="mt-4 movie_info">
              <td>아이디</td>
              <td>
                <input id="id" class="form-control form-control-lg" type="text">
              </td>
              </tr>
              <tr>
                <td>이름</td>
                <td>
                  <input id="name" class="form-control form-control-lg" type="text">
                </td>
              </tr>
              <tr>
                <td>이메일</td>
                <td>
                  <input id="email" class="form-control form-control-lg" type="email">
                </td>
              </tr>
              <tr>
                <td>생일</td>
                <td>
                  <input id="birthDay" class="form-control form-control-lg" type="date">
                </td>
              </tr>
              <tr>
                <td colspan="2" style="text-align: center;">
                  <button id="button" class="btn btn-secondary me-2" style="font-size:1em;">다음</button>
                </td>
              </tr>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>



